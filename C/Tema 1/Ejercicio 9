// Calcular una potencia dada una base y un exponente leídos por teclado

#include <stdio.h>
#include <math.h>

int main (){

  int a, b, potencia;

  printf("Número 1: ");
  scanf("%d", &a);
  printf("Número 2: ");
  scanf("%d", &b);  

  potencia=pow(a,b);

  printf("El resultado de la potencia de estos números es: %d", potencia);

  return 0; 
}
